/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable UART2-M0";
		compatible = "radxa,rock-5a", "radxa,rock-5b", "radxa,rock-5b-plus", "radxa,rock-5c", "radxa,nx5-io", "radxa,cm5-io", "radxa,cm5-rpi-cm4-io";
		category = "misc";
		exclusive = "GPIO0_B5", "GPIO0_B6";
		description = "Enable UART2-M0.\nOn RADXA ROCK 5A/5C this is TX pin 8 & RX pin 10.\nOn Radxa ROCK 5B this is TX pin 8 & RX pin 10.\nOn Radxa ROCK 5B+ this is TX pin 8 & RX pin 10.\nOn Radxa NX5 IO this is TX pin 8 & RX pin 10.\nOn Radxa CM5 IO this is TX pin 8 & RX pin 10.\nOn Radxa CM5 RPI CM4 IO this is TX pin 8 & RX pin 10.";
	};
};

&uart2 {
	status = "okay";
	pinctrl-0 = <&uart2m0_xfer>;
};

&fiq_debugger {
	status = "disabled";
};
