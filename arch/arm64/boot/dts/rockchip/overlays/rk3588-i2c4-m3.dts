/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable I2C4-M3";
		compatible = "radxa,rock-5a", "radxa,rock-5b-plus", "radxa,rock-5c", "radxa,cm5-rpi-cm4-io";
		category = "misc";
		exclusive = "GPIO1_A2", "GPIO1_A3";
		description = "Enable I2C4-M3.\nOn RADXA ROCK 5A/5C this is SDA pin 23 and SCL pin 24.\nOn RADXA ROCK 5A/5C this is SDA pin 31 and SCL pin 29.\nOn Radxa CM5-RPI-CM4-IO this is SDA pin 40 and SCL pin 12.";
	};
};

&i2c4 {
	status = "okay";
	pinctrl-0 = <&i2c4m3_xfer>;
};
