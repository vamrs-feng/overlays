/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable UART7-M2";
		compatible = "radxa,rock-5a", "radxa,rock-5b", "radxa,rock-5c", "radxa,nx5-io", "radxa,cm5-rpi-cm4-io";
		category = "misc";
		exclusive= "GPIO1_B5", "GPIO1_B4";
		description = "Enable UART7-M2.\nOn RADXA ROCK 5A/5C this is TX pin 22 and RX pin 33.\nOn Radxa ROCK 5B this is TX pin 26 & RX pin 24.\nOn Radxa NX5 IO this is TX pin 26 & RX pin 24.\nOn Radxa CM5 RPI CM4 IO this is TX pin 26 & RX pin 24.";
	};
};

&uart7 {
	status = "okay";
	pinctrl-0 = <&uart7m2_xfer>;
};
