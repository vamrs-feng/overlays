/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable PWM6-M0";
		compatible = "radxa,rock-5a", "radxa,rock-5c", "radxa,cm5-io", "radxa,cm5-rpi-cm4-io";
		category = "misc";
		exclusive = "GPIO0_C7";
		description = "Enable PWM6-M0.\nOn RADXA ROCK 5A/5C this is pin 27.\nOn Radxa CM5 IO this is pin 31.\nOn Radxa CM5 RPI CM4 IO this is pin 5.";
	};
};

&pwm6 {
	status = "okay";
	pinctrl-0 = <&pwm6m0_pins>;
};
