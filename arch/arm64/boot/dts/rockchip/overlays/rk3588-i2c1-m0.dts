/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable I2C1-M0";
		compatible = "radxa,rock-5a", "radxa,rock-5b", "radxa,rock-5b-plus", "radxa,rock-5c", "radxa,nx5-io", "radxa,cm5-io", "radxa,cm5-rpi-cm4-io";
		category = "misc";
		exclusive = "GPIO0_B5", "GPIO0_B6";
		description = "Enable I2C1-M0.\nOn RADXA ROCK 5A/5C this is SDA pin 10 & SCL pin 8.\nOn Radxa ROCK 5B this is SDA pin 10 & SCL pin 8.\nOn Radxa ROCK 5B+ this is SDA pin 10 & SCL pin 8.\nOn Radxa NX5 IO this is SDA pin 10 & SCL pin 8.\nOn Radxa CM5 IO this is SDA pin 10 & SCL pin 8.\nOn Radxa CM5 IO this is SDA pin 10 & SCL pin 8.";
	};
};

&fiq_debugger {
	status = "disabled";
};

&i2c1 {
	status = "okay";
	pinctrl-0 = <&i2c1m0_xfer>;
};
