/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable PWM15-M1";
		compatible = "radxa,rock-5a", "radxa,rock-5b", "radxa,rock-5b-plus", "radxa,rock-5c", "radxa,nx5-io";
		category = "misc";
		exclusive = "GPIO4_B3";
		description = "Enable PWM15-M1.\nOn RADXA ROCK 5A/5C this is pin 11.\nOn Radxa ROCK 5B this is pin 3.\nOn Radxa ROCK 5B+ this is pin 3.\nOn Radxa NX5 IO this is pin 32.";
	};
};

&pwm15 {
	status = "okay";
	pinctrl-0 = <&pwm15m1_pins>;
};
