/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable UART6-M1";
		compatible = "radxa,rock-5a", "radxa,rock-5c";
		category = "misc";
		exclusive= "GPIO1_A1", "GPIO1_A0";
		description = "Enable UART6-M1.\nOn RADXA ROCK 5A/5C this is TX pin 19 and RX pin 21.";
	};
};

&uart6 {
	status = "okay";
	pinctrl-0 = <&uart6m1_xfer>;
};
