/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable UART4-M2";
		compatible = "radxa,rock-5a", "radxa,rock-5b", "radxa,rock-5c", "radxa,nx5-io", "radxa,cm5-rpi-cm4-io";
		category = "misc";
		exclusive= "GPIO1_B3", "GPIO1_B2";
		description = "Enable UART4-M2.\nOn RADXA ROCK 5A/5C this is TX pin 7 & RX pin 29.\nOn Radxa ROCK 5B this is TX pin 23 & RX pin 19.\nOn Radxa NX5 IO this is TX pin 23 & RX pin 19.\nOn Radxa CM5 RPI CM4 IO this is TX pin 23 & RX pin 19.";
	};
};

&uart4 {
	status = "okay";
	pinctrl-0 = <&uart4m2_xfer>;
};
