/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable PWM0-M2";
		compatible = "radxa,rock-5a", "radxa,rock-5b-plus", "radxa,rock-5c", "radxa,cm5-rpi-cm4-io";
		category = "misc";
		exclusive = "GPIO1_A2";
		description = "Enable PWM0-M2.\nOn RADXA ROCK 5A/5C this is pin 23.\nOn Radxa ROCK 5B+ this is pin 31.\nOn Radxa CM5-RPI-CM4-IO this is pin 40.";
	};
};

&pwm0 {
	status = "okay";
	pinctrl-0 = <&pwm0m2_pins>;
};
