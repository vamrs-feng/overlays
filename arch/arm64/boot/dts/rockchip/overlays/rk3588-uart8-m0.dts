/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable UART8-M0";
		compatible = "radxa,rock-5a", "radxa,rock-5c";
		category = "misc";
		exclusive= "GPIO4_B0", "GPIO4_B1";
		description = "Enable UART8-M0.\nOn RADXA ROCK 5A/5C this is TX pin 32 and RX pin 40.";
	};
};

&uart8 {
	status = "okay";
	pinctrl-0 = <&uart8m0_xfer>;
};
