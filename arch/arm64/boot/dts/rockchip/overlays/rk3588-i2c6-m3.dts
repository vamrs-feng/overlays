/dts-v1/;
/plugin/;

/ {
	metadata {
		title = "Enable I2C6-M3";
		compatible = "radxa,rock-5a", "radxa,rock-5c";
		category = "misc";
		exclusive = "GPIO4_B0", "GPIO4_B1";
		description = "Enable I2C6-M3.\nOn RADXA ROCK 5A/5C this is SDA pin 32 and SCL pin 40.";
	};
};

&i2c6 {
	status = "okay";
	pinctrl-0 = <&i2c6m3_xfer>;
};
